<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewController">
   <children>
      <ComboBox fx:id="comboBoxPerson" layoutX="50.0" layoutY="50.0" onAction="#onComboBoxPerson" prefWidth="150.0" promptText="Select a person:" />
      <Button fx:id="btAll" layoutX="268.0" layoutY="50.0" mnemonicParsing="false" onAction="#onBtAllAction" text="All" />
   </children>
</AnchorPane>
